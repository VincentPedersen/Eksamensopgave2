<head>
    <title>My matches page</title>
</head>

<body>
    <h1>Your Matches <3</h1>
    <p>First name: {{first_name}}</p> <br>
    <p>Last name: {{last_name}}</p> <br>
    

{{#if visibilityNext}}
    <form id="formNext" name="formNext" action='/next' method='POST'>
    <input style="visibility:visible"type="submit" id="next" name="next" value="Next">
    </form>
{{/if}}
{{#if visibilityPrevious}}
    <form action='/previous' method='POST'>
    <input type="submit" id="submit" value="Previous">
    </form>
{{/if}}

    <form action='/seeFullmatch' method='POST'>
    <input type="submit" id="submit" value="See matches profile">
    </form>

    <form action='/deleteMatch' method='POST'>
    <input type="submit" id="submit" value="Delete Match">
    </form>
 
    <form action='/homepage' method='POST'>
    <input type="submit" id="submit" value="Go back to homepage">
    </form>
    
    

    

    
</body>